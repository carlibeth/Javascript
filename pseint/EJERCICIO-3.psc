Proceso AREA_RECTANGULO
	Definir altura, base, area Como Real;
	
	Escribir "Ingresa los siguientes datos para ayudarte a calcular el area de tu rectangulo";
	
	Escribir "ALTURA en m2";
	Leer altura;
	
	Escribir "BASE en m2";
	Leer base;
	
	area=altura*base;
	
	Escribir "El area de tu rectangulo es " area " metros cuadrados";
	
	
	
FinProceso
