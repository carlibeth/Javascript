Proceso AREA_TRIANGULO
	Definir base, altura, area Como Real;;
	
	Escribir "Calculandoel area de cualquier triangulo";
	
	Escribir "Ingrese la base en centimetros";
	Leer base;
	
	Escribir "Ingrese la altura en centimetros";
	Leer altura;
	
	area=(base*altura)/2;
	
	Escribir "El area de esta triangulo es " area  " centimetros";
	
	
FinProceso
