Proceso Convertir
	Definir cantidad_dolares, cantidad_soles, cambio Como Real;
	cambio=0.29;
	Escribir "Que cantidad de soles disponibles";
	Leer cantidad_soles;
	
	Escribir "1 sol equivale a 0,29 dolares";
	
	cantidad_dolares=cantidad_soles*cambio;
	Escribir "con " cantidad_soles "  soles usted puede adquirir " cantidad_dolares " dolares";
	
FinProceso
